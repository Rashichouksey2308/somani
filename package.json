{
  "dependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@ramonak/react-progress-bar": "^5.0.0",
    "@reduxjs/toolkit": "^1.6.2",
    "axios": "^0.19.0",
    "bootstrap": "^4.6.1",
    "chart.js": "^3.7.1",
    "compression": "^1.7.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "express": "^4.16.2",
    "fs": "^0.0.1-security",
    "helmet": "^3.20.0",
    "humps": "^2.0.1",
    "js-cookie": "^3.0.1",
    "next": "12.1.6",
    "next-themes": "^0.2.0",
    "popper.js": "^1.16.1",
    "prettier": "^2.6.2",
    "prop-types": "^15.7.2",
    "react": "^18.1.0",
    "react-bootstrap": "^2.4.0",
    "react-chartjs-2": "^4.2.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.5",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "sass": "^1.51.0",
    "styled-components": "^5.3.1",
    "styled-normalize": "^8.0.6",
    "use-dark-mode": "^2.3.1",
    "yarn": "^1.22.18"
  },
  "name": "nextjs-redux-starter",
  "version": "1.0.0",
  "main": "server/index.js",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "babel-core": "7.0.0-bridge.0",
    "@babel/eslint-parser": "^7.5.4",
    "babel-jest": "^25.1.0",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "jest": "^25.1.0",
    "jquery": "^3.6.0",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.1",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^18.1.0",
    "rimraf": "^3.0.0",
    "snazzy": "^8.0.0",
    "standard": "^13.0.1"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "build": "NODE_ENV=development next build",
    "test:ci": "jest --maxWorkers=2 --ci",
    "test": "jest --watch",
    "start": "nodemon -w server server/index.js",
    "serve": "NODE_ENV=development node server/index.js",
    "clean": "rimraf node_modules/.cache .next",
    "now-start": "yarn serve",
    "lint:fix": "standard --fix --verbose | snazzy",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "lint": "next lint"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "yarn lint:fix",
      "git add"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "engines": {
    "node": ">= 10"
  },
  "jest": {
    "modulePaths": [
      "./src",
      "./src/test"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  },
  "repository": "CodementorIO/nextjs-redux-starter",
  "bugs": "https://github.com/CodementorIO/nextjs-redux-starter/issues",
  "author": "Ian Wang <gogoenwon@gmail.com> (https://github.com/IanWang)",
  "license": "ISC",
  "description": "Opinionated Next.js starter with Express, Redux, styled-components, and Jest."
}
